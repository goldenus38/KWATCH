{
  "name": "@kwatch/server",
  "version": "1.1.0",
  "private": true,
  "description": "KWATCH 백엔드 서버",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "lint": "eslint src/ --ext .ts",
    "db:generate": "prisma generate",
