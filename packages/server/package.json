    "@types/node": "^20.11.0",
    "@types/nodemailer": "^7.0.9",
    "@types/pngjs": "^6.0.5",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^3.0.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.10.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0",
    "vitest": "^3.0.0"
