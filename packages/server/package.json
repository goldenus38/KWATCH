    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "axios": "^1.13.5",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.68.0",
    "cheerio": "^1.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemailer": "^8.0.1",
    "pixelmatch": "^5.3.0",
    "playwright": "^1.41.0",
    "pngjs": "^7.0.0",
    "sharp": "^0.33.0",
    "socket.io": "^4.7.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0"
